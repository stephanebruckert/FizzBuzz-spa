<section id="fizzbuzzapp">
  <header id="header">
    <h1>FizzBuzz</h1>
    <input type="text" id="search" placeholder="What number are you looking for?" />
  </header>

  <section id="main">
    <ul id="number-list">
      {{#each model as |list|}}
      <li>
        <label>{{list.number}}</label>
      </li>
      {{/each}}
    </ul>
  </section>

  <footer id="footer">
    <button id="previous">
      <strong>Previous</strong> 100
    </button>

    <button id="next">
      <strong>Next</strong> 100
    </button>
  </footer>
</section>
